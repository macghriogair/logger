{
    "name": "macghriogair/logger",
    "description": "Simple logging service",
    "type": "php",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Mac Gregor",
            "email": "macghriogair@gmx.net"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "squizlabs/php_codesniffer": "2.*",
        "phploc/phploc": "dev-master",
        "sebastian/phpcpd": "2.0.*@dev",
        "pdepend/pdepend": "2.2.2",
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
      "psr-4": {
        "Macghriogair\\Logger\\" : "src/"
      }
    },
    "autoload-dev": {
      "classmap": ["tests/"]
    }
}
